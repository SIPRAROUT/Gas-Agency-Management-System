            bool canWrite1 = mw1?.PERMISSION ?? false;
        }
        $(document).on('click', '.details-control2', function () {
            var tr = $(this).closest('tr');
            var row = $('#Tbl_Mstr_ConsumerDetail_View').DataTable().row(tr);
            if (row.child.isShown()) {
                // This row is already open - close it
                row.child.hide();
                tr.removeClass('shown');
                tr.find('.details-control2').css('background-image', 'url("../images/details_open.png")');
            } else {
                // Open this row
                row.child(format(row.data())).show();
                tr.addClass('shown');
                tr.find('.details-control2').css('background-image', 'url("../images/details_close.png")');
            }
        });
        // Use JavaScript variables to store the values of canRead and canWrite
         var canReadAccess = @Html.Raw(Json.Encode(canRead1));
        var canWriteAccess = @Html.Raw(Json.Encode(canWrite1));
        // Trigger click event on the "VIEW" tab if read access is enabled
        if (canReadAccess && !canWriteAccess) {
        $('#tab2').click();
        }
        $('#tab1').click(function () {
            ClearConsumer();
            $('#ddl_CategoryType').focus();

        });
        setTimeout(function () {
            initializeDataTable();
        }, 10);
        var currentId = "";
        var fileInputId = "";
        var debounceTimer;
        $('form').keypress(function (e) {
            if (e.which === 13) {
                e.preventDefault(); // Prevent the default Enter key behavior (e.g., submitting a form)
                if ($('#btn_cancel').is(':focus')) {
                    e.stopPropagation();
                    ClearConsumer(); // Call ClearSupplier if the clear button is focused
                }
                else {
                    SupplierIUDOnEnter(); // Call SupplierIUDOnEnter for form submission
                }
            }
        });
        var currentDate = new Date();
        var formattedCurrentDate = currentDate.getDate() + '/' + (currentDate.getMonth() + 1) + '/' + currentDate.getFullYear();
        // Set the default value for the datepicker input
        $('#dt_ConsumerJoiningDate').val(formattedCurrentDate);
        //-------------------
        function handleFileUpload(input, hdnext, hdnimage, imgElement, allowedExtensions) {
            if (input.files && input.files[0]) {
                var reader = new FileReader();
                var file = input.files[0];
                var ext = file.name.split('.').pop().toLowerCase();
                // Check if the selected file has the allowed extension
                if (allowedExtensions.includes(ext)) {
                    hdnext.val(ext);
                    if (ext === 'pdf') {
                        renderPdfPreview(file, imgElement);
                    } else {
                        reader.onload = function (e) {
                            hdnimage.val(e.target.result);
                            imgElement.attr('src', e.target.result);
                        };
                        reader.readAsDataURL(file);
                    }
                } else {
                    $(input).focus();
                    Swal.fire({
                        icon: 'info',
                        text: 'Please select a file with  format ' + allowedExtensions.join(' ')
                    });
                    $(input).val(''); // Clear the file input
                }
            }
        }
        function renderPdfPreview(pdfFile, imgElement, scale = 2) {
            var fileReader = new FileReader();
            fileReader.onload = function () {
                var typedarray = new Uint8Array(this.result);
                pdfjsLib.getDocument({ data: typedarray }).promise.then(function (pdfDoc) {
                    pdfDoc.getPage(1).then(function (page) {
                        var canvas = document.createElement('canvas');
                        var context = canvas.getContext('2d');
                        // Increase the canvas dimensions based on the scale
                        var viewport = page.getViewport({ scale: scale });
                        canvas.height = viewport.height;
                        canvas.width = viewport.width;
                        var renderContext = {
                            canvasContext: context,
                            viewport: viewport
                        };
                        // Increase the scale for a clearer image
                        page.render(renderContext).promise.then(function () {
                            var imageData = canvas.toDataURL('image/png');
                            hdnimage2.val(imageData);
                            imgElement.attr('src', imageData);
                        });
                    });
                });
            };
            fileReader.readAsArrayBuffer(pdfFile);
        }
        // Image Upload
        $(document).on('click', '#img1', function () {
            $("#Photo_Upload").click();
        });
        $("#Photo_Upload").on('change', function () {
            handleFileUpload(this, $("#hdnext1"), $("#hdnimage1"), $('#img1'), ['jpg', 'jpeg', 'png']);
        });
        // Document Upload
        $(document).on('click', '#img2', function () {
            $("#Document_Upload").click();
        });
        $("#Document_Upload").on('change', function () {
            handleFileUpload(this, $("#hdnext2"), $("#hdnimage2"), $('#img2'), ['jpg', 'jpeg', 'png', 'pdf']);
        });
        BindSelect2();
        $('#ddl_CategoryType').focus();
    });
     var isInsert = true; // Add this flag to track whether it's an insert or update
function updateConsumerNo() {
    // Check if it's an insert operation
    if (isInsert) {
        // Get the selected category ID
        var categoryId = $("#ddl_CategoryType").val();
        console.log('Selected Category ID:', categoryId);
        // Make an AJAX request to fetch the Consumer No based on the selected category
        $.ajax({
            url: '@Url.Action("AutoCunsumerCode", "Master")',
            type: 'GET',
            data: { Catgry_Id: categoryId },
            success: function (consumerNo) {
                // Update the Consumer No input field with the fetched value
                $("#txt_ConsumerNo").val(consumerNo);
            },
            error: function () {
                // Handle error if needed
                console.error('Error fetching Consumer No');
            },
        });
    }
}
    $("#ddl_CategoryType").change(function () {

        updateConsumerNo();
    });
