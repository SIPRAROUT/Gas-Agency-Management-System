    function toggleTabs(tabId) {
        if (tabId === 'content1') {
            debugger;
            $('#content1').show();
            $('#txt_consumerName').focus();
            $('#content2').hide();
            $('#ddl_CategoryType').prop('disabled', false).val('');
            $("#ddl_CategoryType").change(function () {
                updateConsumerNo();
            });
            debugger;
        } else if (tabId === 'content2') {
            setTimeout(function () {
                if ($("#btn_submit").text() == "UPDATE") {
                    ClearConsumer();
                    $("#btn_submit").text("SUBMIT")
                }
                initializeDataTable();
                $('#content1').hide();
                $('#content2').show();
            }, 0);
        }
